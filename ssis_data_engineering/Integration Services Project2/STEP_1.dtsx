      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{53938A73-5341-4342-8846-1D8D904DA35C}"
          SQLTask:SqlStatementSource="IF OBJECT_ID('[Sigma_Connected].[dbo].[Cleaned_Interactions]', 'U') IS NOT NULL&#xA;    DROP TABLE [Sigma_Connected].[dbo].[Cleaned_Interactions];&#xA;&#xA;SELECT TOP (1000)&#xA;    [CustomerID],&#xA;    CASE &#xA;        WHEN ISDATE([Timestamp]) = 1 THEN&#xA;            FORMAT(CAST([Timestamp] AS DATETIME), 'yyyy-MM-dd HH:mm:ss')&#xA;        ELSE NULL&#xA;    END AS [Timestamp_ISO],&#xA;    LOWER([Channel]) AS [Channel],&#xA;    [Direction],&#xA;    LOWER([Status]) AS [Status],&#xA;    CASE &#xA;        WHEN ISNUMERIC([Duration]) = 1 THEN CAST([Duration] AS INT)&#xA;        ELSE 0&#xA;    END AS [Duration_Clean]&#xA;INTO [Sigma_Connected].[dbo].[Cleaned_Interactions]&#xA;FROM [Sigma_Connected].[dbo].[OLE DB Destination]&#xA;WHERE CustomerID != ' ' &#xA;  AND [Timestamp] != 'invalid_date'; --&#xA;--select * from [Sigma_Connected].[dbo].[Cleaned_Interactions]&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
